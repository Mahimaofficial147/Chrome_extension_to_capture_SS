{
    "manifest_version":3,
    "name": "Screenshot Extension",
    "version": "1.0",
    "description": "Take a screenshot of current page.",
    "permissions":["activeTab","tabs"],
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "48":"icon.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["popup.js"]
        }
    ]
}


